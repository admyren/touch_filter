#ifndef STM32_UART_H_
#define STM32_UART_H_

#define USART_1_ADDR 0x4001 1000
#define USART_2_ADDR 0x4000 4400
#define USART_3_ADDR 0x4000 4800
#define UART_4_ADDR 0x4000 4C00
#define UART_5_ADDR 0x4000 5000
#define USART_6_ADDR 0x4001 1400

#endif /* STM32_UART_H_ */
