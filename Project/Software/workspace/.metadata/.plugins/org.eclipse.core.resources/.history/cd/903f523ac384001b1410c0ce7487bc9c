#include "gpio.h"

uint32_t gpio_init(gpio_device_t gpio_dev, gpio_config_t gpio_conf)
{
   uint32_t ret = 0;
   gpio_dev->MODER;
   gpio_dev->OTYPER;
   gpio_dev->OSPEEDR;
   gpio_dev->PUPDR;
   gpio_dev->LCKR;
   gpio_dev->AFR[0];
   gpio_dev->AFR[1];

   return(ret);
}

